<!doctype html>
<html lang="ja">
<head>
  <meta charset="utf-8">
  <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <meta name="fragment" content="!">
  <link rel="manifest" href="/manifest.json">
  <meta name="theme-color" content="#7be5f8">
  <title>{{fragment.title}}</title>
  <meta name="description" content="{{fragment.description}}">
  <meta name="twitter:title" content="{{fragment.title}}">
  <meta name="twitter:description" content="{{fragment.description}}">
  <meta property="og:title" content="{{fragment.title}}">
  <meta property="og:description" content="{{fragment.description}}">
</head>
<body>
<header>
  ahomu-web
</header>
<main id="container">

</main>
<footer>
  copyright aho.mu
</footer>

{{#if polyfillRequired}}
<script src="/polyfill.js" charset="utf-8"></script>
{{/if}}
<%= htmlWebpackPlugin.files.js.map(js =>
`<script src="/${htmlWebpackPlugin.options.path.basename(js)}" charset="utf-8"></script>
`
).join('') %>
</body>
</html>
